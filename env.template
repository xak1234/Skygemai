# Required API Keys for Skygemai
# Copy this file to .env and fill in your API keys

# X.AI Grok API Key (for strategic decision making)
# Get your API key from: https://console.x.ai/
XAI_API_KEY=your_xai_api_key_here

# DeepSeek API Key (for task execution)
DEEPSEEK_API_KEY=your_deepseek_api_key_here

# Optional: Gemini API Key (if you want to use Gemini as well)
GEMINI_API_KEY=your_gemini_api_key_here

# OpenAI API Key
# Get your API key from: https://platform.openai.com/api-keys
OPENAI_API_KEY=your_openai_api_key_here

# Claude API Key
# Get your API key from: https://console.anthropic.com/
CLAUDE_KEY=your_claude_api_key_here

# Genikit URL (if using custom Genikit service)
GENIKIT_URL=your_genikit_url_here

# Firebase Configuration
FIREBASE_CREDS_JSON=your_firebase_credentials_json_here
FIREBASE_DB_URL=your_firebase_database_url_here

# Server Configuration
PORT=10000
PYTHON_VERSION=3.11 